<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="TextAppearance.SuperNestedLayout" parent="android:TextAppearance">
        <item name="android:textColor">@android:color/white</item>
        <item name="android:textColorHint">?android:textColorHint</item>
        <item name="android:textColorHighlight">?android:textColorHighlight</item>
        <item name="android:textColorLink">?android:textColorLink</item>
        <item name="android:textSize">14sp</item>
    </style>
    <style name="TextAppearance.Design.CollapsingLayout.Expanded" parent="TextAppearance.SuperNestedLayout">
        <!--<item name="android:textColor">?android:attr/textColorPrimary</item>-->
        <item name="android:textColor">@android:color/white</item>
        <item name="android:textSize">34dp</item>
    </style>
    <style name="TextAppearance.Design.CollapsingLayout.Collapsed" parent="TextAppearance.SuperNestedLayout">
        <item name="android:textColor">@android:color/white</item>
        <item name="android:textSize">20dp</item>
    </style>
</resources>