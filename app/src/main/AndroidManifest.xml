<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.github.ytjojo.supernestedlayout.demo">
    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name="com.github.ytjojo.supernestedlayout.demo.MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity
            android:name="com.github.ytjojo.supernestedlayout.demo.WithCoordinatorLayoutActivity"
            android:theme="@style/AppTheme.NoActionBar">
        </activity>
        <activity
            android:name="com.github.ytjojo.supernestedlayout.demo.ListActivity"
            android:theme="@style/AppTheme.NoActionBar">
        </activity>
        <activity
            android:name="com.github.ytjojo.supernestedlayout.demo.WebActivity"
            android:theme="@style/AppTheme.NoActionBar">
        </activity>
        <activity
            android:name="com.github.ytjojo.supernestedlayout.demo.NestScorllActivity"
            android:theme="@style/AppTheme.NoActionBar">
        </activity>
        <activity
            android:name="com.github.ytjojo.supernestedlayout.demo.JianshuActivity"
            android:theme="@style/AppTheme.NoActionBar">
        </activity>
        <activity
            android:name="com.github.ytjojo.supernestedlayout.demo.ExistUntilOneNest"
            android:theme="@style/AppTheme.NoActionBar">
        </activity>
        <activity
            android:name="com.github.ytjojo.supernestedlayout.demo.noheader.NoHeaderTwoNest"
            android:theme="@style/AppTheme.NoActionBar">
        </activity>
        <activity
            android:name="com.github.ytjojo.supernestedlayout.demo.CollapingLayoutTwoNeestedActivity"
            android:theme="@style/AppTheme.NoActionBar">
        </activity>
        <activity
            android:name="com.github.ytjojo.supernestedlayout.demo.RecyclerViewActvity"
            android:theme="@style/AppTheme.NoActionBar">
        </activity>
        <activity
            android:name="com.github.ytjojo.supernestedlayout.demo.ViewPagerRefreshActivity"
            android:theme="@style/AppTheme.NoActionBar">
        </activity>
    </application>

</manifest>